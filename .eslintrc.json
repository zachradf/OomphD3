{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "warn",
    "no-use-before-define": "off",
    "comma-dangle": ["error", "always-multiline"],
    "import/extensions": "off",
    "import/prefer-default-export": "off"
  }
}